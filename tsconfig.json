{
  "compilerOptions": {
    "module": "es6",
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "Node",
    "downlevelIteration": true
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*", "@types"]
}
